{
    "name": "jollyblume/state-machine",
    "description": "State machine for jollyblume/workflow",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jolly Blume",
            "email": "jollyblume2@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "jollyblume/workflow-common": "@dev",
        "jollyblume/marking-store": "@dev",
        "symfony/property-access": "^4.2",
        "symfony/workflow": "^4.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "psr/log": "~1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "JBJ\\Workflow\\StateMachine\\": "lib"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JBJ\\Workflow\\StateMachine\\Tests\\": "tests"
        }
    }
}
